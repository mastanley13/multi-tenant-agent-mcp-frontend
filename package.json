{
  "private": true,
  "name": "ghl-agents-integration",
  "version": "1.0.0",
  "description": "GoHighLevel AI Agents Integration with Modern Web Interface",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "tsx --require dotenv/config backend/src/index.ts",
    "server:stub": "cross-env NODE_ENV=development tsx watch backend/src/index.ts",
    "client": "cd client && npm run dev",
    "build": "npm run build:client && npm run build:server",
    "build:server": "cd backend && npm install && tsx build src/index.ts",
    "build:client": "cd client && npm install && npm run build",
    "start": "pnpm-sync && pnpm -r --parallel --stream start",
    "start:production": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "start:server": "cd backend && node dist/index.js",
    "start:client": "cd client && npm start",
    "postinstall": "npm run prisma:generate",
    "prisma:generate": "prisma generate --schema=./client/prisma/schema.prisma",
    "prisma:migrate:deploy": "cd client && prisma migrate deploy",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prisma:studio": "prisma studio",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "@openai/agents": "^0.0.6",
    "@prisma/client": "^5.8.0",
    "@upstash/ratelimit": "^1.2.0",
    "@upstash/redis": "^1.23.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "helmet": "^8.0.0",
    "prom-client": "^15.0.1",
    "socket.io": "^4.8.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.2",
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^8.0.0",
    "prisma": "^5.8.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "keywords": [
    "gohighlevel",
    "ai",
    "agents",
    "chat",
    "crm"
  ],
  "author": "Your Name",
  "license": "MIT"
}
