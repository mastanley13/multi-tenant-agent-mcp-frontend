{
  "private": true,
  "name": "ghl-agents-integration",
  "version": "1.0.0",
  "description": "GoHighLevel AI Agents Integration with Modern Web Interface",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "tsx watch server.ts",
    "client": "cd client && npm run dev",
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsx build server.ts",
    "build:client": "cd client && npm run build",
    "start": "node dist/server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@openai/agents": "^0.0.6",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "socket.io": "^4.8.1",
    "cors": "^2.8.5",
    "helmet": "^8.0.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/express": "^5.0.0",
    "@types/cors": "^2.8.17",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "concurrently": "^9.1.0"
  },
  "keywords": [
    "gohighlevel",
    "ai",
    "agents",
    "chat",
    "crm"
  ],
